#include<iostream>
using namespace std;
int main(){
	int n1,n2;
	float res;
	cout<<"enter number1";
	cin>>n1;
	cout<<"enter number2";
	cin>>n2;
	res=n1+n2;
	cout<<"addition"<<res<<endl;
	res=n1-n2;
	cout<<"subtraction"<<res<<endl;
	res=n1*n2;
	cout<<"multiplication"<<res<<endl;
	res=n1/n2;
	cout<<"division"<<res<<endl;
	return 0;
}
